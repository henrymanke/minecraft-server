{
  "namespace": "social_interactions_textures_dx",

  "background": {
    "type": "panel",
    "controls": [
      {
        "texture": {
          "type": "image",
          "layer": 1,
          "tiled": true, // Only iF the texture is nineslice type.
          "tiled_scale": "$sprite_tiled_scale",
          "texture": "vdx/animated_rgb_ui_textures/social_interactions/background",
          "size": [ "100%", "100%" ],
          "variables": [
            { "requires": "($social_interactions_background_resolution = '1x')",  "$sprite_tiled_scale": [ 1, 1 ] },
            { "requires": "($social_interactions_background_resolution = '2x')",  "$sprite_tiled_scale": [ 0.5, 0.5 ] },
