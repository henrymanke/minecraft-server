            { "requires": "($social_interactions_background_resolution = '32x')", "$sprite_tiled_scale": [ 0.03125, 0.03125 ] }
          ]
        }
      },
      { "border_color_base@CrisXolt_rgb_dialog_bg.rgb_bg_border_social_interactions_color_base": { "layer": 2 } }
    ]
  },

